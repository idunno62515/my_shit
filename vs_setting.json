{
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "window.menuBarVisibility": "default",
  "workbench.activityBar.visible": true,
  "workbench.sideBar.location": "left",
  "workbench.statusBar.visible": true,
  "workbench.colorCustomizations": {
    "editorError.foreground": "#ff000088",
    "editorWarning.foreground": "#00000000",
    "editorInfo.foreground": "#00000000",
    "editor.background": "#111111",
    "sideBar.background": "#141414",
    "activityBar.background": "#212121",
    // "titleBar.activeBackground": "#ff0000",
    "editor.lineHighlightBackground": "#1c1c1c",
    "statusBar.background": "#ad07adb0",
    "statusBar.foreground": "#fff",
    "statusBar.noFolderBackground": "#303030",
    "statusBar.debuggingBackground": "#ad5113c0",
    //cursor
    "editorCursor.foreground": "#ffffffad"
  },
  "editor.tokenColorCustomizations": {
    "[One Dark Theme]": {
      "textMateRules": [
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#CFD7E6"
          },
        },
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "",
            // "foreground": "#E5C07B",
            // "foreground": "#E06C75"
          },
        }
      ]
    },
    "[One Dark (Sublime)]": {
      "textMateRules": [
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#d45b5b"
          },
        },
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "",
            // "foreground": "#E5C07B",
            // "foreground": "#E06C75"
          },
        },
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "",
            // "foreground": "#E5C07B",
            // "foreground": "#E06C75"
          },
        }
      ]
    },
    "[Monokai]": {
      "textMateRules": [
        {
          "scope": "entity.name.type",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "support.type",
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[One Monokai]": {
      "textMateRules": [
        {
          "name": "Keyword",
          "scope": [
            "keyword",
            "keyword.operator.logical",
            "keyword.operator.constructor"
          ],
          "settings": {
            "foreground": "#c062dd",
            "fontStyle": ""
          }
        },
        {
          "scope": "string.unquoted",
          "settings": {
            "foreground": "#cccccce5",
            "fontStyle": ""
          }
        }
      
       
      ]
    },
    "[Monokai Vibrant]": {
      "textMateRules": [
        {
          "name": "Language Variable",
          "scope": "variable.language",
          "settings": {
            "foreground": "#FF3F4F",
            "fontStyle": ""
          }
        },
        {
          "name": "Keyword",
          "scope": [
            "keyword",
            "keyword.operator.logical",
            "keyword.operator.constructor"
          ],
          "settings": {
            "foreground": "#e24cc2",
            "fontStyle": ""
          }
        },
        {
          "name": "Keyword Operator",
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#FF3F4F",
            "fontStyle": ""
          }
        },
        {
          "name": "Storage",
          "scope": "storage",
          "settings": {
            "foreground": "#FF3F4F",
            "fontStyle": ""
          }
        },
        {
          "name": "Function argument",
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#FF9700"
          }
        },
        {
          "name": "Markup: Emphasis",
          "scope": "markup.italic, markup.italic.markdown",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "comment",
          "settings": {
            "fontStyle": ""
          }
        },
      ]
    },
  },
  "markdown.preview.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "C_Cpp.updateChannel": "Google",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0}",
  "markdown.preview.lineHeight": 1.5,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "files.associations": {
    "*.js": "javascript"
  },
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "python.formatting.provider": "autopep8",
  "window.zoomLevel": 0,
  "editor.wordWrap": "on",
  "prettier.useTabs": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  //   "solidity.compileUsingRemoteVersion": "0.6.0",
  "[csharp]": {
    "editor.defaultFormatter": "Leopotam.csharpfixformat"
  },
  "editor.detectIndentation": false,
  "editor.foldingStrategy": "indentation",
  "workbench.tree.indent": 16,
  "editor.fontFamily": "'Fira Mono', Consolas, 'Courier New', monospace",
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "workbench.iconTheme": "material-icon-theme",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "debug.console.fontSize": 12,
  //bracket
  "bracketPairColorizer.showHorizontalScopeLine": false,
  "bracketPairColorizer.showVerticalScopeLine": false,
  "editor.fontWeight": "100",
  "files.autoSave": "onFocusChange",
"editor.cursorSmoothCaretAnimation": true,
"editor.tabSize": 2,
"terminal.explorerKind": "external",
"editor.cursorBlinking": "phase",
"editor.fontSize": 12,
"workbench.colorTheme": "One Monokai"
}
